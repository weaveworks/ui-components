{
  "name": "weaveworks-ui-components",
  "version": "0.1.43",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:theme": "babel src/theme --out-dir lib/theme --ignore *.test.js",
    "compile:utils": "babel src/utils --out-dir lib/utils --ignore *.test.js",
    "compile:components": "babel src/components --out-dir lib/components --ignore *.test.js,example.js",
    "compile:index": "babel src/index.js --out-file lib/index.js",
    "install": "yarn compile:theme && yarn compile:utils && yarn compile:components && yarn compile:index",
    "test": "jest",
    "start": "node bin/dev-server",
    "debug": "node debug ./node_modules/.bin/jest -i",
    "build": "RELEASE=true rm -rf dist && webpack --config webpack.config.js",
    "lint-js": "eslint src docs",
    "lint": "yarn lint-js",
    "ci": "yarn lint && yarn test",
    "s3": "aws s3 sync ./dist/ s3://weaveworks-ui-components --acl public-read",
    "copy:images": "cp -R src/images dist/images",
    "release": "yarn lint && yarn test && yarn build && npm version patch && git push origin --follow-tags && yarn copy:images && yarn s3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weaveworks/ui-components.git"
  },
  "author": "Weaveworks",
  "license": "UNLICENSED",
