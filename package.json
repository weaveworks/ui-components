    "polished": "^1.7.0",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.8",
